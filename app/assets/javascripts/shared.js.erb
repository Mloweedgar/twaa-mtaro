$(function () {
     window.urlParam = function (name) {
        var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
        if (results == null) {
            return 'en';
        }
        else {
            return results[1] || 0;
        }
    }
});
